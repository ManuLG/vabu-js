{
  "name": "vabu-js",
  "version": "0.0.4",
  "description": "Easy JS validation using the builder pattern",
  "author": "Manuel Lorenzo",
  "homepage": "https://github.com/ManuLG/vabu-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuLG/vabu-js"
  },
  "license": "ISC",
  "keywords": [
    "typescript",
    "npm",
    "package",
    "boilerplate"
  ],
  "main": "dist/cjs/vabu-js.js",
  "module": "dist/esm/vabu-js.js",
  "browser": "dist/browser/vabu-js.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs",
    "test": "jest --collect-coverage test/*"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.1",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}
